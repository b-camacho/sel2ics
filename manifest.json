{
  "manifest_version": 2,
  "name": "sel2ical",
  "version": "1.0",
  "description": "Adds a context menu item for highlighted text",
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "contextMenus",
    "https://api.anthropic.com/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
